            </tdk_zipcode_resource>
        </models>
        <blocks>
            <tdk_zipcode>
                <class>TDK_Zipcode_Block</class>
            </tdk_zipcode>
        </blocks>
        <helpers>
            <tdk_zipcode>
                <class>TDK_Zipcode_Helper</class>
            </tdk_zipcode>
        </helpers>
        <resources>
            <tdk_zipcode_setup>
                <setup>
                    <module>TDK_Zipcode</module>
                </setup>
            </tdk_zipcode_setup>
        </resources>
    </global>
